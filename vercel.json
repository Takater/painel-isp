{
    "builds": [
        {
            "src": "djangoproject/wsgi.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "/static/.*",
            "dest": "djangoproject/frontend/build/static"
        },
        {
            "src": "/(.*)",
            "dest": "djangoproject/wsgi.py"
        }
    ]
}