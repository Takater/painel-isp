{
    "builds": [
        {
            "src": "djangoproject/wsgi.py",
            "use": "@vercel/python"
        },
        {
            "src": "build.sh",
            "use": "@vercel/static-build",
            "config": {"distDir": "djangoproject/frontend/build/static/"}
        }
    ],
    "routes": [
        {
            "src": "/static/(.*)",
            "dest": "djangoproject/frontend/build/static/$l"
        },
        {
            "src": "/(.*)",
            "dest": "djangoproject/wsgi.py"
        }
    ]
}